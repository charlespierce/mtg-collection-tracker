{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "jsx": "react",
        "noEmitHelpers": true,
        "importHelpers": true,
        "moduleResolution": "node",
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true
    },
    "include": [
        "src/**/*"
    ]
}
